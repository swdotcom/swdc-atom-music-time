{
    "name": "music-time",
    "displayName": "Music Time",
    "main": "./lib/Activator",
    "version": "1.9.4",
    "scripts": {
        "test": "mocha"
    },
    "description": "Music Time is an open source plugin that curates and launches playlists for coding right from your editor.",
    "keywords": [
        "music-time",
        "music",
        "time",
        "spotify",
        "software",
        "software.com",
        "code",
        "productivity",
        "activity",
        "manager",
        "keystrokes",
        "calendar",
        "analytics"
    ],
    "repository": "https://github.com/swdotcom/swdc-atom-music-time",
    "license": "Reproduction of this material is strictly forbidden unless prior written permission is obtained from Software.co Technologies, Inc.",
    "engines": {
        "atom": ">=1.0.0 <2.0.0"
    },
    "configSchema": {
        "showGitMetrics": {
            "title": "Show Music Time git metrics",
            "description": "Music Time weekly git commit metrics",
            "type": "boolean",
            "default": false,
            "order": 2
        }
    },
    "dependencies": {
        "@slack/web-api": "^5.0.1",
        "axios": "^0.19.0",
        "clipboardy": "^2.1.0",
        "cody-music": "2.9.25",
        "crypto": "^1.0.1",
        "jquery": "^3.5.0",
        "moment-timezone": "^0.5.25",
        "open": "6.3.0"
    },
    "devDependencies": {
        "mocha": "^6.2.2",
        "typescript": "^2.7.2"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Music Time",
            "properties": {
                "showGitMetrics": {
                    "type": "boolean",
                    "default": false,
                    "description": "Git metrics"
                },
                "showWeeklyRanking": {
                    "type": "boolean",
                    "default": false,
                    "description": "Weekly Ranking"
                }
            }
        }
    }
}
